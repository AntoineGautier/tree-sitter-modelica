{
  "name": "tree-sitter-modelica",
  "version": "0.2.2",
  "description": "Modelica grammar for tree-sitter",
  "main": "bindings/node",
  "types": "bindings/node",
  "license": "SEE LICENSE IN OSMC-License.txt",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "generate": "tree-sitter generate",
    "build": "npm run generate && tree-sitter build --wasm --docker .",
    "test": "tree-sitter test",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip",
    "sync-metadata": "node scripts/sync-metadata.js",
    "version": "npm run sync-metadata && git add tree-sitter.json"
  },
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "prebuildify": "^6.0.0",
    "tree-sitter-cli": "0.25.10"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ]
}
