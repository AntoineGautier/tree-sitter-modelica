{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "modelica",
      "camelcase": "Modelica",
      "title": "Modelica",
      "scope": "source.modelica",
      "file-types": [
        "modelica"
      ],
      "injection-regex": "^modelica$",
      "class-name": "TreeSitterModelica"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Modelica Language",
    "authors": [
      {
        "name": "reituag"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-modelica"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}