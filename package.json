{
  "name": "tree-sitter-modelica",
  "version": "0.2.1",
  "description": "Modelica grammar for tree-sitter",
  "main": "bindings/node",
  "types": "bindings/node",
  "license": "SEE LICENSE IN OSMC-License.txt",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "tree-sitter generate && tree-sitter build --wasm --docker .",
    "test": "tree-sitter test",
    "preinstall": "tree-sitter generate",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "dependencies": {
    "node-addon-api": "^8.4.0",
    "node-gyp-build": "^4.8.4"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "node-fetch": "^3.3.2",
    "prebuildify": "^6.0.1",
    "prettier": "^3.6.2",
    "tree-sitter-cli": "0.25.6",
    "web-tree-sitter": "^0.25.6"
  },
  "tree-sitter": [
    {
      "scope": "source.modelica",
      "file-types": [
        "mo"
      ]
    }
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ]
}
